variant: fcos
version: 1.3.0
storage:
  trees:
    - path: /etc/systemd/system
      local: service/letsencrypt/systemd/
systemd:
  units:
    - name: letsencrypt-dns-register@.service
      enabled: true
    - name: letsencrypt-dns-renew@.service
      enabled: true
    - name: letsencrypt-dns-renew@.timer
      enabled: true
