variant: fcos
version: 1.3.0
storage:
  trees:
    - path: /etc/systemd/system
      local: service/nginx/systemd/
systemd:
  units:
    - name: nginx-ingress.service
      enabled: true
    - name: nginx-ingress-http@.service
      enabled: true
    - name: nginx-static@.service
      enabled: true
    - name: nginx-php@.service
      enabled: true
