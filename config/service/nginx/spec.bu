variant: fcos
version: 1.3.0
storage:
  trees:
    - path: /etc/systemd/system
      local: service/nginx/systemd/
systemd:
  units:
    - name: nginx.service
      enabled: true
    - name: nginx-proxy-http@.service
      enabled: true
    - name: nginx-serve-static@.service
      enabled: true
    - name: nginx-serve-php@.service
      enabled: true
