variant: fcos
version: 1.3.0
storage:
  trees:
    - path: /etc/coreos-home-server/dovecot
      local: service/dovecot/
    - path: /etc/systemd/system
      local: service/dovecot/systemd/
systemd:
  units:
    - name: dovecot-firstboot.target
      enabled: true
    - name: dovecot-fts-optimize.timer
      enabled: true
    - name: dovecot.service
      enabled: true
